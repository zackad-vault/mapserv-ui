{
  "name": "mapserver-ui",
  "version": "0.1.4-alpha",
  "description": "A Frontend for Mapserver WMS",
  "main": "src/js/index.js",
  "scripts": {
    "test": "bundlesize",
    "purify-css": "purifycss dist/css/main.css dist/index.html dist/js/app.js -m -o dist/css/main.css",
    "build": "webpack-dev-server",
    "build:prod": "NODE_ENV=production webpack -p && npm run purify-css"
  },
  "bundlesize": [
    {
      "path": "./dist/js/app.js",
      "maxSize": "120 kB"
    },
    {
      "path": "./dist/css/main.css",
      "maxSize": "20 kB"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zackad/mapserv-ui.git"
  },
  "keywords": [
    "wms",
    "map"
  ],
  "author": "zackad",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zackad/mapserv-ui/issues"
  },
  "homepage": "https://github.com/zackad/mapserv-ui#readme",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.6.1",
    "bundlesize": "^0.15.3",
    "clean-webpack-plugin": "^0.1.16",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.5",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.5.3",
    "purify-css": "^1.2.5",
    "sass-loader": "^6.0.6",
    "webpack": "^3.4.1",
    "webpack-dev-server": "^2.6.1"
  },
  "dependencies": {
    "font-awesome": "^4.7.0",
    "normalize-url": "1.9.1",
    "ol": "4.2.0",
    "skeleton-css": "^2.0.4",
    "vue": "2.5.9"
  }
}
